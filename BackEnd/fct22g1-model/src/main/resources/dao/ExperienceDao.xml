<?xml version="1.0" encoding="UTF-8"?>
<JdbcEntitySetup
    xmlns="http://www.ontimize.com/schema/jdbc"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
    catalog="" schema="${mainschema}" table="experiences"
    datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
    <DeleteKeys>
        <Column>id</Column>
    </DeleteKeys>
    <UpdateKeys>
        <Column>id</Column>
    </UpdateKeys>
    <GeneratedKey>id</GeneratedKey>
    
    <Queries>
        <Query id="experiences_that_they_are_not_in_a_box">
            <AmbiguousColumns>
                <AmbiguousColumn name="exp_id" prefix="exp"
                                 databaseName="id"/>
                <AmbiguousColumn name="expBox_id" prefix="expBox"
                                                  databaseName="id"/>
            </AmbiguousColumns>
            <Sentence>
                <![CDATA[
                  select
                   #COLUMNS#
                  from
                   ${mainschema}.experiences
                   where id not in (
                   select distinct idexp
                   from ${mainschema}.experience_experience_box
                   #WHERE#
                 )
                ]]>
            </Sentence>
        </Query>
        
    </Queries>
</JdbcEntitySetup>